#include <iostream>
#include <map>

int main() {
  std::map<std::string, int> m;

  std::pair<std::string, int> tmp{"alma", 4};
  m.insert(tmp);

  m["alma"] = 4;
  m["korte"] = 5;

  std::cout << "Value of \"edit\": " << m["edit"] << '\n';

  std::cout << "Size: " << m.size() << '\n';

  for (std::map<std::string, int>::iterator it = m.begin();
       it != m.end(); ++it) {
    std::cout << it->first << ' ' << it->second << '\n';
  }
}
